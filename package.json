{
  "name": "tv-show-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "format": "npx prettier --write \"src/**/*.{ts,vue,css}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,vue,css}\"",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "pinia": "2.1.7",
    "swiper": "11.1.14",
    "vue": "3.5.24",
    "vue-router": "4.3.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "@types/node": "24.10.0",
    "vite": "7.2.2",
    "@vitejs/plugin-vue": "6.0.1",
    "vitest": "4.0.10",
    "@vitest/ui": "4.0.10",
    "vue-tsc": "3.1.3",
    "@vue/tsconfig": "0.8.1",
    "@vue/test-utils": "2.4.5",
    "@vue/compiler-sfc": "3.5.24",
    "jsdom": "24.0.0",
    "prettier": "3.2.5"
  }
}
